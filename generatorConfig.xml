<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- 数据库驱动包位置 (需要根据自己的路径修改) -->
	<classPathEntry location="E:/Myproject/ojdbc6-11.2.0.1.0.jar" />
	<context id="Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 数据库链接URL、用户名、密码 （定制修改） -->
		<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
			connectionURL="jdbc:oracle:thin:@192.168.0.37:1521:ORCL" userId="twweb"
			password="1234"></jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- 生成模型的包名和位置 （targetPackage和targetProject根据自己要求修改） -->
		<javaModelGenerator targetPackage="com.telewave.twweb.firstds.entity" targetProject="tw_Web\src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 生成的映射文件包名和位置 （targetPackage和targetProject根据自己要求修改） -->
		<sqlMapGenerator targetPackage="com.telewave.twweb.firstds.mapper" targetProject="tw_Web\src\main\java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- 生成DAO的包名和位置 （targetPackage和targetProject根据自己要求修改） -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.telewave.twweb.firstds.dao" targetProject="tw_Web\src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<!-- 要生成那些表(更改tableName和domainObjectName就可以，多个表的话复制table标签在后边排列即可) -->
	 	<table schema='TwWeb' tableName="SYS_USER" domainObjectName="User"
			enableCountByExample="true" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="true" />
